body
  p 0 40px 40px

  &.root.index
    #container
      p 2em 0 1em

  .content-side
    float right
    w 33%
    ml 1%
    .text-block
      bb none
      pt 0

  .content-main
    float right
    w 66%
    .section
      ml 0 !important
      mr 0 !important

  .text-block.large 
    m 0
    strong
      pb .75em
      mb .75em
      bb 1px solid #ddd

  #footer
    p 2em 0
    .content-main
      m 0

